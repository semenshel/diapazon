#include <iostream>
using namespace std;

int main(){
    int start = 0;
    int finish = 0;

    int sum = 0;

    


    cout << "Введите начало диапазона" << endl;
    cin >> start;
    cout << "Введите конец диапазона" << endl;
    cin >> finish;

    int i = start;

    while(i <= finish){
        if(i % 2 != 0){
            cout  << i << " ";
            sum += i;
        }
        i++;
        
        


    }
    cout << "\nСумма нечетных чисел в диапазоне от " << start << "до " << finish << endl;
    cout << sum << endl;
    return 0;
}
